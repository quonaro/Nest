import ./nestfile into nesty

var APP_NAME = vscode-nestfile-support
var ENV = development

build:
    desc: Build VS Code extension
    script: |
        npm run compile

watch:
    desc: Watch and rebuild on changes
    script: |
        npm run watch

dev:
    desc: Development helpers

    default:
        desc: Open README
        script: |
            echo "Open README.md in VS Code for details"



