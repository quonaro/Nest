{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": [
        "string"
      ]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": [
        "string"
      ]
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ],
    [
      "'",
      "'"
    ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*([A-Za-z0-9_]+\\s*\\(|[A-Za-z0-9_]+\\s*):\\s*$"
  },
  "folding": {
    "offSide": true
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([A-Za-z0-9_\\-\\.\\/\\(\\)\\[\\]]+|@[A-Za-z0-9_]+|\\{\\{[^}]+\\}\\})"
}